<Window x:Class="SGHE.Cliente.ExperienciasEducativasDocente"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SGHE.Cliente"
        mc:Ignorable="d"
        Title="Experiencias Educativas Docente" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="70"/>
        </Grid.RowDefinitions>
        <Label Content="Tus Experiencias Educativas" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" FontSize="25" FontWeight="Bold"/>
        <ListBox x:Name="lbExperiencias" Grid.Row="1" Margin="133,53,133,-201" />
        <Label Content="Nota: Selecciona una experiencia para ver los detalles" HorizontalAlignment="Left" Margin="133,9,0,0" Grid.Row="1" VerticalAlignment="Top"/>
    </Grid>
</Window>
